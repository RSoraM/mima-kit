{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "resolveJsonModule": true,
    "types": [
      "@vitest/browser/matchers"
    ],
    "strict": true,
    "strictNullChecks": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
